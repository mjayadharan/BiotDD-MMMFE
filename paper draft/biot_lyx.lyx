\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\|u_{h}^{n+1}-u_{h}^{n}\|+\|\gamma_{h}^{n+1}-\gamma_{h}^{n}\| & \le C_{inf-sup}\sup_{0\ne\tau\in\mathbb{X}_{h}}\frac{\inp[u_{h}^{n+1}-u_{h}^{n}]{\dvr{\tau}}+\inp[\gamma_{h}^{n+1}-\gamma_{h}^{n}]{\tau}}{\|\tau\|_{H(div;\Omega,\mathbb{M})}}\\
 & \le C_{inf-sup}\sup_{0\ne\tau\in\mathbb{X}_{h}}\frac{\inp[A(\sigma_{h}^{n+1}-\sigma_{h}^{n})]{\tau}+\inp[A\a(p_{h}^{n}-p_{h}^{n-1})I]{\tau}}{\|\tau\|_{H(div;\Omega,\mathbb{M})}}.
\end{align*}

